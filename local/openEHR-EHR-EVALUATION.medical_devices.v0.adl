	EVALUATION[at0000] matches {    -- Medical_devices
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- ICD_in_place
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Yes
									at0004]    -- No
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- ICD_deactivated
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006,    -- Yes, permanently deactivated
									at0007,    -- Yes, temporarily deactivated
									at0008]    -- No
								}
							}
						}
					}
				}
			}
		}
	}

