definition
	EVALUATION[at0000] matches {    -- Death summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0116] occurrences matches {0..1} matches {    -- D&T_death
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0093] occurrences matches {0..1} matches {    -- Circumstances_death
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0109] occurrences matches {0..*} matches {    -- Place_of_death
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {    -- Age_at_death
						value matches {
							DV_DURATION matches {
								value matches {PYMWDTHM/|>=PT0M|}
							}
						}
					}
					ELEMENT[at0117] occurrences matches {0..1} matches {    -- Person(s)_present
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0105] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0009] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0103] occurrences matches {0..*} matches {    -- Information source(s)
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

