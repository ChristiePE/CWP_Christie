definition
	EVALUATION[at0000] matches {    -- Prognosis
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Likely prognosis
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Currently stable. Prognosis around a year.
									at0004,    -- Unstable with declining condition, may have repeated admission or co-morbidities. Prognosis of months.
									at0005,    -- Deteriorating, but they are not in the terminal phase of their illness. Prognosis of weeks.
									at0006]    -- In the terminal phase of their illness and likely to die within the next few days. Prognosis of days.
								}
							}
						}
					}
				}
			}
		}
	}

